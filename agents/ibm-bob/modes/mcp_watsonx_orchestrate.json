{
    "mcpServers": {
        "wxo-docs": {
            "command": "uvx",
            "args": [
                "mcp-proxy",
                "--transport",
                "streamablehttp",
                "https://developer.watson-orchestrate.ibm.com/mcp"
            ],
            "alwaysAllow": [
                "SearchIbmWatsonxOrchestrateAdk"
            ],
            "disabled": false
        },
        "orchestrate-adk": {
            "command": "uvx",
            "args": [
                "--with",
                "ibm-watsonx-orchestrate==1.14.1",
                "ibm-watsonx-orchestrate-mcp-server==1.14.1"
            ],
            "env": {
                "WXO_MCP_WORKING_DIRECTORY": "<full path to your code project directory>"
            },
            "alwaysAllow": [
                "list_agents",
                "export_agent",
                "get_tool_template",
                "list_tools",
                "list_toolkits",
                "list_knowledge_bases",
                "check_knowledge_base_status",
                "list_connections",
                "list_voice_configs",
                "list_models",
                "check_version"
            ],
            "disabled": false
        }
    }
}
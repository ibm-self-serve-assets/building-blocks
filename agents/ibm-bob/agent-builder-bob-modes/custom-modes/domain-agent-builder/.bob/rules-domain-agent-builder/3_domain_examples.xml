<?xml version="1.0" encoding="UTF-8"?>
<domain_examples>
  <overview>
    Comprehensive examples of how to adapt the agent template for different domains.
    Use these as reference when building domain-specific agents. Each example demonstrates
    how to customize entity models, capabilities, and communications for specific industries.
  </overview>

  <domain name="healthcare">
    <description>Healthcare and medical services domain</description>
    
    <entity_model>
      <entity_type>patients</entity_type>
      <tiers>
        <tier>Standard Care</tier>
        <tier>Premium Care</tier>
        <tier>VIP Care</tier>
        <tier>Concierge Care</tier>
      </tiers>
      <key_fields>
        <field>patient_id</field>
        <field>name</field>
        <field>date_of_birth</field>
        <field>insurance_provider</field>
        <field>primary_physician</field>
        <field>care_tier</field>
        <field>last_visit_date</field>
        <field>chronic_conditions</field>
        <field>medications</field>
      </key_fields>
    </entity_model>

    <capabilities>
      <capability>Appointment scheduling and management</capability>
      <capability>Medical record access and retrieval</capability>
      <capability>Prescription refill requests</capability>
      <capability>Health information and education</capability>
      <capability>Insurance and billing inquiries</capability>
    </capabilities>

    <communication_types>
      <type>Appointment reminders</type>
      <type>Test result notifications</type>
      <type>Prescription refill alerts</type>
      <type>Wellness tips and health education</type>
      <type>Billing and insurance updates</type>
    </communication_types>

    <sample_tools>
      <tool name="get_patient_data">
        <description>Retrieve patient profiles and medical history</description>
      </tool>
      <tool name="schedule_appointment">
        <description>Book or modify patient appointments</description>
      </tool>
      <tool name="request_prescription_refill">
        <description>Submit prescription refill requests</description>
      </tool>
      <tool name="generate_health_communication">
        <description>Create personalized health messages</description>
      </tool>
    </sample_tools>

    <sample_data_structure>
      <csv_example><![CDATA[
patient_id,name,email,phone,care_tier,insurance_provider,primary_physician,last_visit_date,chronic_conditions
PAT001,Jane Smith,jane.smith@email.com,555-0101,Premium Care,Blue Cross,Dr. Johnson,2026-01-15,Diabetes Type 2
PAT002,John Doe,john.doe@email.com,555-0102,Standard Care,Aetna,Dr. Williams,2026-01-20,Hypertension
PAT003,Mary Johnson,mary.j@email.com,555-0103,VIP Care,United Healthcare,Dr. Brown,2026-01-25,None
      ]]></csv_example>
    </sample_data_structure>
  </domain>


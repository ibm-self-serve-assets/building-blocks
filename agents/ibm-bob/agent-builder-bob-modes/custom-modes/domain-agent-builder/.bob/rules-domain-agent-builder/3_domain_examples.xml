  <customization_patterns>
    <pattern name="tier_naming">
      <guideline>Choose tier names that resonate with your domain and reflect value progression</guideline>
      <examples>
        <example domain="healthcare">Standard Care, Premium Care, VIP Care, Concierge Care</example>
        <example domain="retail">Basic, Silver, Gold, Platinum</example>
        <example domain="education">Free, Basic, Premium, Enterprise</example>
        <example domain="finance">Basic Banking, Preferred, Private Banking, Wealth Management</example>
        <example domain="hospitality">Standard, Preferred, Elite, Ambassador</example>
        <example domain="manufacturing">Standard, Preferred, Strategic, Exclusive</example>
        <example domain="saas">Free, Starter, Professional, Enterprise</example>
        <example domain="membership">Bronze, Silver, Gold, Diamond</example>
      </examples>
    </pattern>

    <pattern name="entity_naming">
      <guideline>Use domain-appropriate terms for entities that match industry conventions</guideline>
      <examples>
        <example domain="healthcare">patients</example>
        <example domain="retail">customers</example>
        <example domain="education">students or learners</example>
        <example domain="finance">clients or customers</example>
        <example domain="hospitality">guests</example>
        <example domain="manufacturing">suppliers or partners</example>
        <example domain="real_estate">clients or buyers</example>
        <example domain="insurance">policyholders</example>
        <example domain="logistics">shippers or clients</example>
        <example domain="legal">clients</example>
        <example domain="saas">users or subscribers</example>
        <example domain="nonprofit">members or donors</example>
      </examples>
    </pattern>

    <pattern name="communication_tone">
      <guideline>Adapt communication style to domain expectations and user relationships</guideline>
      <examples>
        <example domain="healthcare">Professional, caring, informative, empathetic</example>
        <example domain="retail">Friendly, promotional, engaging, enthusiastic</example>
        <example domain="education">Supportive, motivational, educational, encouraging</example>
        <example domain="finance">Professional, trustworthy, advisory, conservative</example>
        <example domain="hospitality">Warm, personalized, service-oriented, welcoming</example>
        <example domain="legal">Formal, precise, professional, confidential</example>
        <example domain="technology">Innovative, helpful, technical, clear</example>
      </examples>
    </pattern>

    <pattern name="capability_focus">
      <guideline>Prioritize capabilities that address core domain workflows and pain points</guideline>
      <examples>
        <example domain="healthcare">Scheduling, records access, prescription management</example>
        <example domain="retail">Order tracking, recommendations, loyalty management</example>
        <example domain="education">Enrollment, progress tracking, resource access</example>
        <example domain="finance">Account management, transactions, financial planning</example>
        <example domain="logistics">Shipment tracking, route optimization, delivery scheduling</example>
      </examples>
    </pattern>
  </customization_patterns>

  <cross_domain_patterns>
    <pattern name="tiered_service">
      <description>Most domains benefit from tiered service levels that differentiate value</description>
      <implementation>
        <step>Define 3-4 clear tiers with meaningful names</step>
        <step>Assign specific benefits to each tier</step>
        <step>Create tier-specific communications and experiences</step>
        <step>Track tier-based metrics and performance</step>
        <step>Implement upgrade paths and incentives</step>
      </implementation>
    </pattern>

    <pattern name="personalization">
      <description>All domains benefit from personalized interactions based on entity data</description>
      <implementation>
        <step>Collect entity preferences and characteristics</step>
        <step>Track interaction history and behavior</step>
        <step>Use data for personalized recommendations</step>
        <step>Customize communications with relevant details</step>
        <step>Adapt tone and content to entity profile</step>
      </implementation>
    </pattern>

    <pattern name="proactive_engagement">
      <description>Agents should proactively engage entities with timely, relevant information</description>
      <implementation>
        <step>Send timely reminders and notifications</step>
        <step>Provide relevant updates and alerts</step>
        <step>Offer personalized recommendations</step>
        <step>Celebrate milestones and achievements</step>
        <step>Anticipate needs based on patterns</step>
      </implementation>
    </pattern>

    <pattern name="data_driven_insights">
      <description>Leverage data to provide valuable insights and analytics</description>
      <implementation>
        <step>Calculate relevant metrics and KPIs</step>
        <step>Identify trends and patterns</step>
        <step>Generate actionable insights</step>
        <step>Provide comparative analysis</step>
        <step>Support decision-making with data</step>
      </implementation>
    </pattern>

    <pattern name="multi_channel_support">
      <description>Support interactions across multiple communication channels</description>
      <implementation>
        <step>Design for chat, email, and other channels</step>
        <step>Maintain consistent experience across channels</step>
        <step>Adapt content format to channel</step>
        <step>Track interactions across channels</step>
        <step>Provide seamless channel transitions</step>
      </implementation>
    </pattern>
  </cross_domain_patterns>

  <domain_selection_guide>
    <guideline>When building an agent for a new domain, consider these factors:</guideline>
    <factors>
      <factor>Industry-specific terminology and conventions</factor>
      <factor>Regulatory and compliance requirements</factor>
      <factor>Typical user workflows and pain points</factor>
      <factor>Data structures and relationships</factor>
      <factor>Communication preferences and expectations</factor>
      <factor>Integration requirements with existing systems</factor>
      <factor>Security and privacy considerations</factor>
    </factors>
  </domain_selection_guide>
</domain_examples>

